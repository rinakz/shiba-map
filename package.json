{
  "name": "shiba-map",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "vite --host --force",
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 1",
    "lint-has-warnings": "eslint \"**/*.ts[x]\" --max-warnings=0",
    "lint-all": "eslint --fix .",
    "lint-cache": "eslint --cache --fix .",
    "ts:check": "tsc --skipLibCheck --noEmit",
    "preview": "vite preview",
    "prepare": "husky install",
    "prettier all": "npx prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rinakz/shiba-map"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@pbe/react-yandex-maps": "^1.2.5",
    "embla-carousel-autoplay": "^8.5.2",
    "embla-carousel-react": "^8.5.2",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^7.4.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.86.0",
    "typescript": "^5.8.2",
    "vite-tsconfig-paths": "^4.3.2",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.11",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.0",
    "typescript": "^4.7.2",
    "vite": "^5.2.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint-has-warnings",
      "eslint --cache --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
